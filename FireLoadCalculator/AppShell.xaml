<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FireLoadCalculator.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FireLoadCalculator"
    xmlns:views="clr-namespace:FireLoadCalculator.Views"
    Shell.FlyoutBehavior="Locked">

    <FlyoutItem Title="Materials" Route="materials">
        <ShellContent ContentTemplate="{DataTemplate views:AllMaterialsPage}"/>
    </FlyoutItem>
    <FlyoutItem Title="Rooms" Route="rooms">
        <ShellContent ContentTemplate="{DataTemplate views:AllRoomsPage}" />
    </FlyoutItem>

</Shell>
